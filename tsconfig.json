{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": ["dom", "es2015"],
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["cypress/**/*.ts", "src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  